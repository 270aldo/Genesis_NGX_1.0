# GENESIS_oficial_BETA Frontend Environment Configuration Example
# =============================================================
# Copy this file to .env.local and fill in your actual values
# DO NOT commit .env.local to version control

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000/ws
VITE_A2A_WS_URL=ws://localhost:8001

# Authentication
VITE_AUTH_ENABLED=true

# Feature Flags
VITE_VOICE_ENABLED=true
VITE_HYBRID_INTELLIGENCE_ENABLED=true
VITE_REAL_TIME_ANALYSIS_ENABLED=true
VITE_DEBUG_MODE=true

# Development
VITE_MOCK_MODE=false
VITE_LOG_LEVEL=debug

# Environment
VITE_ENVIRONMENT=development
VITE_APP_VERSION=1.0.0

# Timeouts and Limits
VITE_API_TIMEOUT=30000
VITE_WS_RECONNECT_ATTEMPTS=10
VITE_MAX_FILE_SIZE=10485760

# Cache Settings
VITE_CACHE_DURATION=300000
VITE_OFFLINE_ENABLED=true

# Supabase (Optional - if you want to use Supabase directly from frontend)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# ElevenLabs Voice API (Optional)
VITE_ELEVENLABS_API_KEY=your-elevenlabs-api-key